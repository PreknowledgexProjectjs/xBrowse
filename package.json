{
  "name": "xbrowseapp",
  "productName": "XBrowseApp",
  "version": "0.1.7-betabeta1",
  "description": "new",
  "author": "Px Team <preknowledge@gmail.com>",
  "homepage": "https://x.preknowledge.in",
  "keywords": [
    "electron",
    "browser",
    "navigation",
    "tab",
    "addressbar"
  ],

  "main": "main/main.js",
  "author": "pxproj",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/"
  },
  "scripts": {
    "start:control": "npx babel src --out-dir main/renderer/js",
    "start": "electron main/main.js --enable-transparent-visuals",
    "runjs": "nodemon devLib/start.js --watch src",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "p.x.browse",
    "nsis": {
      "include": "src/installer.nsh"
    },
    "mac": {
      "category": "public.app-category.utilities",
      "icon": "icons/icon.icns"
    },
    "win": {
      "asar": true,
      "target": "nsis",
      "icon": "icons/icon.ico"
    },
   
    "fileAssociations": {
      "ext": [
        "xext",
        "html",
        "htm"
      ],
      "name": "PxExtLauncher",
      "description": "Access Internet"
    }
  },
  "dependencies": {
    "axios": "^0.25.0",
    "classnames": "^2.2.6",
    "data-store": "^4.0.3",
    "electron-is-dev": "^2.0.0",
    "electron-log": "^3.0.7",
    "electron-window-state": "^5.0.3",
    "file-url": "^2.0.2",
    "jquery": "^3.6.0",
    "nodescriptencryption": "^1.0.4",
    "react": "^16.8.6",
    "react-desktop": "^0.3.9",
    "react-dom": "^16.8.6",
    "react-loading-icons": "^1.0.8",
    "socket.io": "^4.4.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.8",
    "@babel/core": "^7.16.7",
    "@babel/preset-env": "^7.16.8",
    "@babel/preset-react": "^7.16.7",
    "@chakra-ui/babel-plugin": "^1.0.2",
    "cli-color": "^2.0.1",
    "electron": "^16.0.7",
    "electron-builder": "^22.14.5",
    "nodemon": "^2.0.15"
  }
}
